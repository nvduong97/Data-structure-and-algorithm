package OnTap;

import java.io.FileInputStream;
import java.io.FileNotFoundException;
import java.util.Scanner;
import java.util.Stack;

public class Stack2 {
	static boolean checkCharacter(char s1, char s2) {
		if (s1 == '(' && s2 == ')')
			return true;
		if (s1 == '<' && s2 == '>')
			return true;
		if (s1 == '[' && s2 == ']')
			return true;
		if (s1 == '{' && s2 == '}')
			return true;
		return false;
	}
	// Nếu bạn chép code mà không chạy đúng thì B1: là soi code, nếu ko được thì sang B2: xóa đi code lại
	static boolean areCharacter(char s[]) {
		Stack<Character> st = new Stack<Character>();
		for(int i=0; i<s.length; i++) {
			if(s[i]=='(' || s[i]=='<' || s[i]=='[' || s[i]=='{') {
				st.push(s[i]);
			}
			if(s[i]==')' || s[i]=='>' || s[i]==']' || s[i]=='}') {
				if(st.isEmpty()) { // chỗ này empty() hay isEmpty() vấn AC
					return false;
				}
				else if(!checkCharacter(st.pop(), s[i])) {
					return false;
				}
			}
		}
		if(st.isEmpty()) {
			return true;
		}
		return false;
	}

	public static void main(String[] args) {
		try {
			System.setIn(new FileInputStream("inputStack.txt"));
		} catch (FileNotFoundException e) {
			// TODO Auto-generated catch block
			e.printStackTrace();
		}
		Scanner sc = new Scanner(System.in);
		for (int tc = 1; tc <= 13; tc++) {
			int n = sc.nextInt();
			char s[] = new char[n];
			s = sc.next().toCharArray();
			if (areCharacter(s)) {
				System.out.println("#" + tc + " 1");
			} else {
				System.out.println("#" + tc + " 0");
			}
		}
	}

}
