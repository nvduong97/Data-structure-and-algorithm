#include<iostream>
using namespace std;
FILE* stream;
int main() {
	freopen_s(&stream,"input.txt", "r", stdin);
	int test, n, i, j, tc;
	cin >> test;
	for (tc = 1; tc <= test; tc++) {
		cin >> n;
		int *a = new int[n + 5];
		for (i = 0; i < n; i++) {
			cin >> a[i];
		}
		int sum = 0;
		for (i = n - 1; i >= 0; i--) {
			for (j = i - 1; j >= 0; j--) {
				if (a[i] > a[j]) {
					sum += a[i] - a[j];
				}
				else if (a[i] <= a[j]) {
					i = j;
				}
				if (j == 0) {
					i = 0;
				}
			}
		}
		cout << "#" << tc << " " << sum << endl;
	}

	return 0;
}
